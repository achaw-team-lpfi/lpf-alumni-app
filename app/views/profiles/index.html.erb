<h1 class="page-header">Alumni Index</h1>
<div id="outPopUp">
  <table class = "table table-striped table-hover">
    <thead>
      <tr>
        <th>Alumni Name</th>
      </tr>
    </thead>
    <tbody>
      <% @profiles.each do |profile| %>
        <tr>
          <td><a href=<%="/profiles/#{profile.id}"%>><%= profile.fullname %></a></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
